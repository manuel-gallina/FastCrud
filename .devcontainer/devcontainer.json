{
	"name": "FastCrud",
	"dockerComposeFile": "docker-compose.yml",
	"service": "api",
	"workspaceFolder": "/workspace",
	"shutdownAction": "stopCompose",
	"containerEnv": {
		"PYTHONPATH": "/workspace/src:${PYTHONPATH}"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"charliermarsh.ruff"
			]
		}
	}
}
